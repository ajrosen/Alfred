				<key>script</key>
				<string>on run argv

set AppleScript's text item delimiters to "\",\""

tell application "GeekTool Helper"
	set r to "{ \"items\": ["

	repeat with g in geeklets
		if (name of g) is not missing value then
			set r to r &amp; " {"
			set r to r &amp; "\"uid\": \"" &amp; (name of g) &amp; "\","
			set r to r &amp; "\"arg\": \"" &amp; (name of g) &amp; "\","
			set r to r &amp; "\"title\": \"" &amp; (name of g) &amp; "\","
			set r to r &amp; "\"subtitle\": \"" &amp; (class of g) &amp; "\","
			set r to r &amp; "\"autocomplete\": \"" &amp; (name of g) &amp; "\","
			set r to r &amp; "\"icon\": { \"path\": \"icon.png\" }"
			set r to r &amp; " }, "
		end if	end repeat

	set r to r &amp; "] }"

	return r
end tell

end run</string>
